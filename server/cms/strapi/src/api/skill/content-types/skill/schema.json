{
  "kind": "collectionType",
  "collectionName": "skills",
  "info": {
    "singularName": "skill",
    "pluralName": "skills",
    "displayName": "Skill",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "requires_cloud_to_run": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "workflows": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::workflow.workflow",
      "mappedBy": "skills"
    },
    "app": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::app.app",
      "inversedBy": "skills"
    },
    "icon": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::uploaded-file.uploaded-file"
    },
    "slug": {
      "type": "string",
      "required": true
    },
    "is_llm_endpoint": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "is_llm_endpoint_and_supports_tool_selection": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "pricing": {
      "type": "json"
    },
    "processing": {
      "type": "enumeration",
      "enum": [
        "local",
        "cloud",
        "localORcloud"
      ],
      "required": true
    },
    "free": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "default_skill_settings": {
      "type": "json"
    }
  }
}
