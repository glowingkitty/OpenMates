    },
    "software": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::software.software",
      "inversedBy": "skills"
    },
    "icon": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::uploaded-file.uploaded-file"
    }
