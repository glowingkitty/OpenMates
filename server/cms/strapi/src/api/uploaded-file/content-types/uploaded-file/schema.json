      "target": "api::team.team"
    },
    "read_access_limited_to_users": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-account.user-account"
    },
    "filename": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "write_access_limited_to_teams": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::team.team"
    },
    "write_access_limited_to_users": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-account.user-account"
    }
